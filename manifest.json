{
    "manifest_version": 3,
    "name": "AdFriend",
    "description": "AdFriend!",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "declarativeNetRequest",
      "storage",
      "tabs",
      "notifications"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icone.png"
    },
    "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icone-128.png"
    }
  }
  